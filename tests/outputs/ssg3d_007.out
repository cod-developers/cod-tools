# x1,x2,x3,x4 -> x,y,z
# x1,-x2,1/2+x3,-x4 -> x,-y,z+0.5
# 1/2+x1,1/2+x2,x3,x4 -> x+0.5,y+0.5,z
# 1/2+x1,1/2-x2,1/2+x3,-x4 -> x+0.5,-y+0.5,z+0.5
nrepreset: 2
representatives:
    x,y,z
    x,-y,z+0.5
nsymops:   4
symops:
    x,y,z
    x,-y,z+0.5
    x+0.5,y+0.5,z
    x+0.5,-y+0.5,z+0.5
inversion: 0
centering: 0,0,0 0.5,0.5,0 
{
   "constraints" => "$alpha == 90 && $gamma == 90",
   "crystal_class" => "monoclinic",
   "hall" => " C -2yc",
   "hermann_mauguin" => "C 1 c 1",
   "ncsym" => [ "x,y,z", "x,-y,z+1/2" ],
   "number" => "9",
   "schoenflies" => "Cs^4",
   "symops" => [ "x,y,z", "x,-y,z+1/2", "x+1/2,y+1/2,z", "x+1/2,-y+1/2,z+1/2" ],
   "universal_h_m" => "C 1 c 1",
},
