# x1,x2,x3,x4 -> x,y,z
# -x2,x1-x2,x3,x4 -> -y,x-y,z
# -x1+x2,-x1,x3,x4 -> -x+y,-x,z
# x2,x1,-x3,-x4 -> y,x,-z
# x1-x2,-x2,-x3,-x4 -> x-y,-y,-z
# -x1,-x1+x2,-x3,-x4 -> -x,-x+y,-z
# -x1,-x2,-x3,-x4 -> -x,-y,-z
# x2,-x1+x2,-x3,-x4 -> y,-x+y,-z
# x1-x2,x1,-x3,-x4 -> x-y,x,-z
# -x2,-x1,x3,x4 -> -y,-x,z
# -x1+x2,x2,x3,x4 -> -x+y,y,z
# x1,x1-x2,x3,x4 -> x,x-y,z
nrepreset: 6
representatives:
    x,y,z
    -y,x-y,z
    -x+y,-x,z
    y,x,-z
    -x,-x+y,-z
    x-y,-y,-z
nsymops:   12
symops:
    x,y,z
    -y,x-y,z
    -x+y,-x,z
    y,x,-z
    -x,-x+y,-z
    x-y,-y,-z
    -x,-y,-z
    y,-x+y,-z
    x-y,x,-z
    -y,-x,z
    x,x-y,z
    -x+y,y,z
inversion: 1
centering: 0,0,0 
{
   "constraints" => "$a == $b && $alpha == 90 && $beta == 90 && $gamma == 120",
   "crystal_class" => "trigonal",
   "hall" => "-P 3 2"",
   "hermann_mauguin" => "P -3 m 1",
   "ncsym" => [ "x,y,z", "-y,x-y,z", "-x+y,-x,z", "y,x,-z", "-x,-x+y,-z", "x-y,-y,-z", "-x,-y,-z", "y,-x+y,-z", "x-y,x,-z", "-y,-x,z", "x,x-y,z", "-x+y,y,z" ],
   "number" => "164",
   "schoenflies" => "D3d^3",
   "symops" => [ "x,y,z", "-y,x-y,z", "-x+y,-x,z", "y,x,-z", "-x,-x+y,-z", "x-y,-y,-z", "-x,-y,-z", "y,-x+y,-z", "x-y,x,-z", "-y,-x,z", "x,x-y,z", "-x+y,y,z" ],
   "universal_h_m" => "P -3 m 1",
},
