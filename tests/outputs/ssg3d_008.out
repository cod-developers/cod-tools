# +x1,+x2,+x3,+x4 -> x,y,z
# -x1,+x2,+x3,1/2-x4 -> -x,y,z
# +x1,-x2,1/2+x3,+x4 -> x,-y,z+0.5
# -x1,-x2,1/2+x3,1/2-x4 -> -x,-y,z+0.5
# -x1,-x2,-x3,-x4 -> -x,-y,-z
# +x1,-x2,-x3,-1/2+x4 -> x,-y,-z
# -x1,+x2,-1/2-x3,-x4 -> -x,y,-z-0.5
# +x1,+x2,-1/2-x3,-1/2+x4 -> x,y,-z-0.5
# 1/2+x1,1/2+x2,+x3,+x4 -> x+0.5,y+0.5,z
# 1/2-x1,1/2+x2,+x3,1/2-x4 -> -x+0.5,y+0.5,z
# 1/2+x1,1/2-x2,1/2+x3,+x4 -> x+0.5,-y+0.5,z+0.5
# 1/2-x1,1/2-x2,1/2+x3,1/2-x4 -> -x+0.5,-y+0.5,z+0.5
# -1/2-x1,-1/2-x2,-x3,-x4 -> -x-0.5,-y-0.5,-z
# -1/2+x1,-1/2-x2,-x3,-1/2+x4 -> x-0.5,-y-0.5,-z
# -1/2-x1,-1/2+x2,-1/2-x3,-x4 -> -x-0.5,y-0.5,-z-0.5
# -1/2+x1,-1/2+x2,-1/2-x3,-1/2+x4 -> x-0.5,y-0.5,-z-0.5
nrepreset: 4
representatives:
    x,y,z
    -x,y,z
    x,-y,z+0.5
    -x,-y,z+0.5
nsymops:   16
symops:
    x,y,z
    -x,y,z
    x,-y,z+0.5
    -x,-y,z+0.5
    x+0.5,y+0.5,z
    -x+0.5,y+0.5,z
    x+0.5,-y+0.5,z+0.5
    -x+0.5,-y+0.5,z+0.5
    -x,-y,-z
    x,-y,-z
    -x,y,-z+0.5
    x,y,-z+0.5
    -x+0.5,-y+0.5,-z
    x+0.5,-y+0.5,-z
    -x+0.5,y+0.5,-z+0.5
    x+0.5,y+0.5,-z+0.5
inversion: 1
centering: 0,0,0 0.5,0.5,0 
{
   "constraints" => "$alpha == 90 && $beta == 90 && $gamma == 90",
   "crystal_class" => "orthorhombic",
   "hall" => "-C 2c 2",
   "hermann_mauguin" => "C m c m",
   "ncsym" => [ "x,y,z", "-x,-y,z+1/2", "x,-y,-z", "-x,y,-z+1/2", "-x,-y,-z", "x,y,-z-1/2", "-x,y,z", "x,-y,z-1/2" ],
   "number" => "63",
   "schoenflies" => "D2h^17",
   "symops" => [ "x,y,z", "-x,-y,z+1/2", "x,-y,-z", "-x,y,-z+1/2", "-x,-y,-z", "x,y,-z-1/2", "-x,y,z", "x,-y,z-1/2", "x+1/2,y+1/2,z", "-x+1/2,-y+1/2,z+1/2", "x+1/2,-y+1/2,-z", "-x+1/2,y+1/2,-z+1/2", "-x+1/2,-y+1/2,-z", "x+1/2,y+1/2,-z-1/2", "-x+1/2,y+1/2,z", "x+1/2,-y+1/2,z-1/2" ],
   "universal_h_m" => "C m c m",
},
#-------------------------
