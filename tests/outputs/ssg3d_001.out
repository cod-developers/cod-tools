# x1,x2,x3,x4 -> x,y,z
# -x1,x2,-x3,-x4+1/2 -> -x,y,-z
# -x1,-x2,-x3,-x4 -> -x,-y,-z
# x1,-x2,x3,x4+1/2 -> x,-y,z
# x1+1/2,x2+1/2,x3,x4 -> x+0.5,y+0.5,z
# -x1+1/2,x2+1/2,-x3,-x4+1/2 -> -x+0.5,y+0.5,-z
# -x1+1/2,-x2+1/2,-x3,-x4 -> -x+0.5,-y+0.5,-z
# x1+1/2,-x2+1/2,x3,x4+1/2 -> x+0.5,-y+0.5,z
nrepreset: 2
representatives:
    x,y,z
    -x,y,-z
nsymops:   8
symops:
    x,y,z
    -x,y,-z
    x+0.5,y+0.5,z
    -x+0.5,y+0.5,-z
    -x,-y,-z
    x,-y,z
    -x+0.5,-y+0.5,-z
    x+0.5,-y+0.5,z
inversion: 1
centering: 0,0,0 0.5,0.5,0 
{
   "constraints" => "$alpha == 90 && $gamma == 90",
   "crystal_class" => "monoclinic",
   "hall" => "-C 2y",
   "hermann_mauguin" => "C 1 2/m 1",
   "ncsym" => [ "x,y,z", "-x,y,-z", "-x,-y,-z", "x,-y,z" ],
   "number" => "12",
   "schoenflies" => "C2h^3",
   "symops" => [ "x,y,z", "-x,y,-z", "-x,-y,-z", "x,-y,z", "x+1/2,y+1/2,z", "-x+1/2,y+1/2,-z", "-x+1/2,-y+1/2,-z", "x+1/2,-y+1/2,z" ],
   "universal_h_m" => "C 1 2/m 1",
},
#-------------------------
