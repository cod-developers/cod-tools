# x1,x2,x3,x4,x5 -> x,y,z
# -x1,-x2,-x3,-x4,-x5 -> -x,-y,-z
# 1/2+x1,1/2+x2,x3,x4,x5 -> x+0.5,y+0.5,z
# 1/2-x1,1/2-x2,-x3,-x4,-x5 -> -x+0.5,-y+0.5,-z
nrepreset: 1
representatives:
    x,y,z
nsymops:   4
symops:
    x,y,z
    x+0.5,y+0.5,z
    -x,-y,-z
    -x+0.5,-y+0.5,-z
inversion: 1
inversion translation: 0, 0, 0
centering: 0,0,0 0.5,0.5,0 
{
   "constraints" => "1",
   "crystal_class" => "triclinic",
   "hall" => "-C 1",
   "hermann_mauguin" => "C -1",
   "ncsym" => [ "x,y,z", "-x,-y,-z" ],
   "number" => "2",
   "schoenflies" => "Ci^1",
   "symops" => [ "x,y,z", "-x,-y,-z", "x+1/2,y+1/2,z", "-x+1/2,-y+1/2,-z" ],
   "universal_h_m" => "C -1",
},
