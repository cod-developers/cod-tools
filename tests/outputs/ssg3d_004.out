# x1,x2,x3,x4 -> x,y,z
# -x1,x2+1/2,-x3,x4+1/2 -> -x,y+0.5,-z
# -x1,-x2,-x3,-x4 -> -x,-y,-z
# x1,-x2+1/2,x3,-x4+1/2 -> x,-y+0.5,z
nrepreset: 2
representatives:
    x,y,z
    -x,y+0.5,-z
nsymops:   4
symops:
    x,y,z
    -x,y+0.5,-z
    -x,-y,-z
    x,-y+0.5,z
inversion: 1
centering: 0,0,0 
{
   "constraints" => "$alpha == 90 && $gamma == 90",
   "crystal_class" => "monoclinic",
   "hall" => "-P 2yb",
   "hermann_mauguin" => "P 1 21/m 1",
   "ncsym" => [ "x,y,z", "-x,y+1/2,-z", "-x,-y,-z", "x,-y-1/2,z" ],
   "number" => "11",
   "schoenflies" => "C2h^2",
   "symops" => [ "x,y,z", "-x,y+1/2,-z", "-x,-y,-z", "x,-y-1/2,z" ],
   "universal_h_m" => "P 1 21/m 1",
},
