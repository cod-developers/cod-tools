    _description.text
;
    This category is parent of all other categories in the
    SIMPLE_HEAD_IMPORT_DIC dictionary.
;
    _name.category_id            CIF_TEST_HEAD_IMPORT
    _name.object_id              CIF_TEST_HEAD_IMPORT_HEAD
    _import.get                  [ { "file":"cif_test.dic"
                                     "save":"CIF_TEST"
                                     "mode":"Full" } ]
save_

