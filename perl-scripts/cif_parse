#! /bin/sh
#!perl -w # --*- Perl -*--
eval 'exec perl -x $0 ${1+"$@"}'
    if 0;
#------------------------------------------------------------------------------
#$Author$
#$Date$ 
#$Revision$
#$URL$
#------------------------------------------------------------------------------
#*
#  Parse a CIF file
#**

use strict;
use lib "./lib/perl5";
use STAR::Parser;
use ShowStruct;

my $filename = shift(@ARGV);
my @data;

@data = STAR::Parser->parse($filename);


for my $dataset (@data) {
    showHash( $dataset );
}
