#! /bin/sh
#!perl -w # --*- Perl -*--
eval 'exec perl -x $0 ${1+"$@"}'
    if 0;
#------------------------------------------------------------------------------
#$Author$
#$Date$ 
#$Revision$
#$URL$
#------------------------------------------------------------------------------
#*
#  Determine a spacegroup from a list of symmetry operators
#  (generators), using the Ralf's (Ralf Grosse-Kunstleve) algorithm.
#**

use strict;
use warnings;

use SGBuilder;

my $spacegroup = new SGBuilder;

while(<>) {
    next if /^#/;
    next if /^\s*$/;
    $spacegroup->insert_symop_string( $_ );
}

$spacegroup->print();
