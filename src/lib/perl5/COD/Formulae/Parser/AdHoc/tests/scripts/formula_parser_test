#! /bin/sh
#!perl -w # --*- Perl -*--
eval 'exec perl -x $0 ${1+"$@"}'
    if 0;
#------------------------------------------------------------------------------
#$Author$
#$Date$ 
#$Revision$
#$URL$
#------------------------------------------------------------------------------
#*
#  Test driver for COD::Formulae::Parser::IUCr.pm module.
#**
use strict;
use warnings;
use COD::Formulae::Parser::AdHoc;

@ARGV = ("-") unless @ARGV;

foreach my $filename ( @ARGV ) {
    my $parser = new COD::Formulae::Parser::AdHoc;
    $parser->Run($filename);
    $parser->PrintFormula;
};
